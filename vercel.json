{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/blog/:slug/index.html",
      "destination": "/blog/:slug/index.html"
    },
    {
      "source": "/blog/:slug",
      "destination": "/blog/:slug/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
