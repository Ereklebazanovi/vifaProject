{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "inventogeo.com"
        }
      ],
      "destination": "https://www.inventogeo.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/blog/:slug/index.html",
      "destination": "/blog/:slug/index.html"
    },
    {
      "source": "/blog/:slug",
      "destination": "/blog/:slug/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
