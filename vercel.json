{
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "destination": "https://vifadigital.ge/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.vifadigital.ge"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|home).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}